{
  "version": 2,
  "builds": [
    {
      "src": "server/",
      "use": "@vercel/node"
    },
    {
      "src": "client/",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ]
}
